PIB<-read.csv(file.choose())##pib de México del 80 al 99
pibts<-ts(PIB,frequency=1,start=1980)

acf(window(pibts,start=1990))# Por lo tanto esta serie tiene ruido blanco

IPC<-read.csv(file.choose())##IPC DIARIO DE 21/02/17 	A 21/03/17 	
ipcts<-ts(IPC,frequency=365,star=c(02,21,2017))
ipcts
x11();acf(window(ipcts,start=c(2,21),end=c(2,31)))

#algunas se salen de la banda de confianza podrían significar caminata aleatoria



